{
  "name": "shipxp-backend",
  "version": "1.0.0",
  "description": "Backend for ShipXP",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "honeycomb:setup": "tsc && node dist/scripts/setupHoneycomb.js",
    "verxio:setup": "tsc && node dist/scripts/setupVerxio.js"
  },
  "dependencies": {
    "@google/genai": "^1.14.0",
    "@google/generative-ai": "^0.24.1",
    "@honeycomb-protocol/edge-client": "latest",
    "@metaplex-foundation/umi": "latest",
    "@metaplex-foundation/umi-bundle-defaults": "latest",
    "@metaplex-foundation/umi-web3js-adapters": "latest",
    "@solana/web3.js": "latest",
    "@verxioprotocol/core": "latest",
    "firebase-admin": "^13.4.0",
    "typescript": "^5"
  },
  "devDependencies": {
    "@types/ace": "^0.0.52",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/node-fetch": "^2.5.12",
    "@types/supertest": "^2.0.11",
    "jest": "^27.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "@metaplex-foundation/umi": "latest",
      "@metaplex-foundation/umi-bundle-defaults": "latest",
      "@metaplex-foundation/umi-web3js-adapters": "latest",
      "@metaplex-foundation/umi-eddsa-web3js": "latest",
      "@metaplex-foundation/umi-public-keys": "latest",
      "@metaplex-foundation/umi-rpc-web3js": "latest",
      "@metaplex-foundation/umi-serializers": "latest",
      "@metaplex-foundation/umi-serializers-core": "latest",
      "@metaplex-foundation/umi-serializers-numbers": "latest",
      "@metaplex-foundation/umi-serializers-encodings": "latest",
      "@metaplex-foundation/umi-http-fetch": "latest",
      "@metaplex-foundation/umi-downloader-http": "latest",
      "@metaplex-foundation/umi-program-repository": "latest",
      "@metaplex-foundation/umi-rpc-chunk-get-accounts": "latest",
      "@metaplex-foundation/umi-signer-wallet-adapters": "latest",
      "@metaplex-foundation/umi-transaction-factory-web3js": "latest",
      "@metaplex-foundation/umi-uploader-irys": "latest"
    }
  }
}
